<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet id="create_ContactSQL_table" author="Beksultan">
        <createTable tableName="contactsql">
            <column name="id" autoIncrement="true" type="bigint">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="date_of_birth" type="varchar(255)">
            </column>
            <column name="first_phone_number" type="varchar(255)">
                <constraints unique="true"/>
            </column>
            <column name="second_phone_number" type="varchar(255)">
                <constraints unique="true"/>
            </column>

        </createTable>
    </changeSet>
</databaseChangeLog>